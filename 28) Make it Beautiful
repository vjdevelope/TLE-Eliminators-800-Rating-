#include <iostream>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        
        int a[n];
        
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
        }
        
        if(a[0]==a[n-1])
        {
            cout<<"NO"<<endl;
        }
        else
        {   
            cout<<"YES"<<endl;
            cout<<a[n-1]<<" "<<a[0]<<" ";
            for(int i=1;i<n-1;i++)
            {
                cout<<a[i]<<" ";
            }
            cout<<endl;
        }
    }

    return 0;
}
