#include <iostream>
using namespace std;
int main()
{ 
    int t;
    cin>>t;
    
    while (t--)
    {
        int n;
        cin>>n;
        
        int a[n];
        
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
        }
        
        int k=n+1;
         for(int i=0;i<n;i++)
        {
          cout<<(k-a[i])<<" ";
        }
        cout<<endl;
    }

    return 0;
}
